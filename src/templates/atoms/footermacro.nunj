{% macro contact() %}
    <div class="contact">
        {{caller()}}
    </div>
{% endmacro %}

{% macro menu() %}
    <ul class="menu">
        {{caller()}}
    </ul>
{% endmacro %}

{% macro socialIcons() %}
    <div class="social-icons">
        {{caller()}}
    </div>
{% endmacro %}

{% macro img(img) %}
    <img src="{{img}}" />
{% endmacro %}

{% macro info(text, src, number, mail) %}
    <p>{{text}}</p>
    <p>P:<span><a src="{{src}}">{{number}}</a></span> | M:<span><a src="{{src}}">{{mail}}</a></span></p>
{% endmacro %}

{% macro icon(icon) %}
    <i data-feather="{{icon}}"></i>
{% endmacro %}

{% macro item(src, item) %}
    <li><a src="{{src}}">{{item}}</a></li>
{% endmacro %}

{% macro title(title) %}
    <li>
        <div>{{title}}</div>
    </li>
{% endmacro %}

{% macro copyright(company, src, theme, text, marcet) %}
    <div>{{company}}<a src="{{src}}">{{themes}}</a>{{text}}<a src="{{src}}">{{marcet}}</a></div>
{% endmacro %}